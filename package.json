{
  "name": "api-one",
  "version": "0.0.1",
  "author": "Niek Janssen",
  "license": "MIT",
  "description": "Attempt at full automation of backand-synchronization and server backend storage. ",
  "homepage": "https://github.com/atasak/MusicOne#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/atasak/ApiOne"
  },
  "bugs": {
    "url": "https://github.com/atasak/ApiOne/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build: windows": "if exist dist\\ rd /s /q dist && npm run build",
    "build:linux": "rm -rf dist && npm run build",
    "build:travis": "tsc",
    "run": "node dist/compiler/runner.js",
    "test": "nyc mocha \"src/**/*.spec.ts\" --opts mocha.opts",
    "test:coverage": "nyc check-coverage --lines 50 --functions 50 --branches 50",
    "coverage": "nyc report",
    "doc": "typedoc --out doc src --exclude src/runtime/lib.bak/**/*"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/index.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "@types/node": "^8.5.2",
    "ts-simple-ast": "^5.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.46",
    "@types/sinon": "^4.1.2",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "sinon": "^4.1.3",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0"
  }
}
